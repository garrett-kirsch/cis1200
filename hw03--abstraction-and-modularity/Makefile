# DO NOT EDIT THIS FILE

SUBMIT      := higherOrder.ml tree.ml setTest.ml listSet.ml treeSet.ml vocab.ml

DEPENDS := \
	assert.ml \
	assert.mli \
	setInterface.ml \
	setInterface.mli \
	higherOrder.ml \
	higherOrder.mli \
	tree.ml \
	tree.mli \
	setTest.ml \
	listSet.ml \
	listSet.mli \
	treeSet.ml \
	treeSet.mli \
	books.txt \
	books.ml \
	books.mli \
	vocab.ml \
	vocab.mli 


HWNAME := hw03

# autogenerated zipfile name with timestamp
ts := $(shell /bin/date "+%Y-%m-%d-%H:%M:%S")
ZIPNAME := $(HWNAME)-submit($(ts)).zip

.PHONY: all runHigherOrder runTree runSetTest runVocab clean zip

all:	higherOrder.native tree.native setTest.native vocab.native

%.native: $(DEPENDS)
	ocamlbuild -pkg unix -tag debug -r $@

runHigherOrder: higherOrder.native
	OCAMLRUNPARAM=b ./higherOrder.native

runTree: tree.native
	OCAMLRUNPARAM=b ./tree.native

runSetTest: setTest.native
	OCAMLRUNPARAM=b ./setTest.native

runVocab: vocab.native
	OCAMLRUNPARAM=b ./vocab.native

zip: $(SUBMIT)
	zip '$(ZIPNAME)' $(SUBMIT)

format:

clean:
	ocamlbuild -clean
	rm -rf *.zip

utop: 
	ocamlbuild -pkg unix assert.cmo books.cmo higherOrder.cmo tree.cmo setInterface.cmo \
	listSet.cmo treeSet.cmo setTest.cmo vocab.cmo
