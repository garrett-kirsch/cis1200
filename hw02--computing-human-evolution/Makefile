# DO NOT EDIT THIS FILE

# Set this to the the name of the top-level executable that should be executed:
MAIN := dnaTest
SUBMIT := dna.ml dnaTest.ml
HWNAME := hw02

# autogenerated zipfile name with timestamp
ts := $(shell /bin/date "+%Y-%m-%d-%H:%M:%S")
ZIPNAME := $(HWNAME)-submit($(ts)).zip

.PHONY: all run clean zip

all:	$(MAIN).native

$(MAIN).native: dna.ml dnaTest.ml helices.ml assert.ml 
	ocamlbuild -tag debug -pkg unix -r $(MAIN).native

run: $(MAIN).native
	OCAMLRUNPARAM=b ./$(MAIN).native


zip: $(SUBMIT)
	zip '$(ZIPNAME)' $(SUBMIT)

format:
clean:
	ocamlbuild -clean
	rm -rf *.zip

utop: 
	ocamlbuild -pkg unix assert.cmo helices.cmo

